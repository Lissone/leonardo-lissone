  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "yarn commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "yarn pretty-quick --staged",
      "yarn eslint --fix --ext"
    ]
  }
